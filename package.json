{
  "name": "hpccloud-kemsu",
  "description": "Virtual platform for running simulations and visualization",
  "license": "ISC",
  "version": "0.9.0",
  "main": "./lib/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dealenx/hpccloud-kemsu.git"
  },
  "scripts": {
    "validate": "prettier --config ./prettier.config.js --list-different \"src/**/*.js\"",
    "reformat": "prettier --config ./prettier.config.js --write \"src/**/*.js\"",
    "install:pyfr": "cp ./node_modules/simput/dist/types/pyfr.js ./dist/simput-pyfr.js",
    "install:nwchem": "cp ./node_modules/simput/dist/types/nwchem.js ./dist/simput-nwchem.js",
    "install:nwchem-neb": "cp ./node_modules/simput/dist/types/nwchem-neb.js ./dist/simput-nwchem-neb.js",
    "install:openfoam": "cp ../simput-kemsu/dist/types/openfoam_tutorials.js ./dist/simput-openfoam-tutorials.js",
    "install:openfoam_cavity": "cp ../simput-kemsu/dist/types/openfoam_cavity.js ./dist/simput-openfoam_cavity.js",
    "install:openfoam_cavity_test": "cp ../simput-kemsu/dist/types/openfoam_cavity_test.js ./dist/simput-openfoam_cavity_test.js",
    "install:openfoamw": "cp ./node_modules/simput/dist/types/openfoam-windtunnel.js ./dist/simput-openfoam-windtunnel.js",
    "install:simput": "npm run install:pyfr && npm run install:nwchem && npm run install:nwchem-neb && npm run install:openfoam && npm run install:openfoamw && npm run install:openfoam_cavity",
    "check": "node bin/version-check.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "prebuild": "npm run check",
    "build": "webpack --progress --colors --mode development",
    "build:debug": "webpack --display-modules --mode development",
    "build:release": "webpack --progress --colors --mode production",
    "build:watch": "webpack --progress --watch --watch-poll --mode development",
    "start": "webpack-dev-server --progress --open",
    "test": "karma start test/karma.all.js",
    "test:redux": "karma start test/karma.redux.js",
    "test:component": "karma start test/karma.component.js",
    "test:debug": "karma start test/karma.all.js --no-single-run --browsers Chrome",
    "codecov": "cat coverage/lcov/lcov.info | ./node_modules/codecov.io/bin/codecov.io.js",
    "commit": "git cz",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "antd": "^4.7.0",
    "commander": "2.13.0",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "monologue.js": "0.3.5",
    "mout": "1.1.0",
    "open": "0.0.5",
    "query-string": "6.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "reselect": "3.0.1",
    "shelljs": "0.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.8.6",
    "axios": "0.18.0",
    "babel-loader": "^8.0.2",
    "bootstrap": "3.3.7",
    "candela": "0.19.1",
    "codecov.io": "0.1.6",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^1.0.0",
    "expect": "1.20.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine": "3.1.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-electron": "5.2.2",
    "karma-jasmine": "1.1.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-tap": "4.1.0",
    "karma-webpack": "2.0.9",
    "kw-doc": "1.1.1",
    "kw-web-suite": "6.0.1",
    "paraviewweb": "3.1.2",
    "postcss-loader": "^3.0.0",
    "prettier": "2.1.2",
    "pvw-visualizer": "3.1.10",
    "react-test-renderer": "16.2.0",
    "redux-actions-assertions": "1.3.0",
    "redux-thunk": "2.2.0",
    "simput": "1.5.0",
    "style-loader": "^0.23.0",
    "svg-sprite-loader": "4.1.6",
    "tap-markdown": "1.2.1",
    "tap-spec": "4.1.1",
    "tape": "4.8.0",
    "tape-catch": "1.0.6",
    "vtk.js": "6.3.14",
    "vuepress": "^1.4.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8",
    "wslink": "0.1.7"
  },
  "bin": {
    "HPCCloud": "./bin/hpc-cloud-cli.js"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
